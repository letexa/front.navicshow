{
  "extends": "../tslint.json",
  "rules": {
    "no-inferrable-types": false,
    "use-life-cycle-interface": false,
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
